[
    {
        "id": "tools",
        "children":
        [
            {
                "caption": "Simple Indent",
                "command": "simple_indent"
            }
        ]
    },
    {
        "id": "preferences",
        "children":
        [
            {
                "id": "package-settings",
                "children":
                [
                    {
                        "caption": "Simple Indent",
                        "children":
                        [
                            {
                                "caption": "Settings – Default",
                                "command": "open_file",
                                "args":
                                {
                                    "file": "${packages}/SimpleIndent/SimpleIndent.sublime-settings"
                                }
                            },
                            {
                                "caption": "Settings – User",
                                "command": "open_file",
                                "args":
                                {
                                    "file": "${packages}/User/SimpleIndent.sublime-settings"
                                }
                            }
                        ]
                    }
                ]
            }
        ]
    }
]